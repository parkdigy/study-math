{
  "name": "study-math",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.ts",
  "scripts": {
    "dev": "concurrently --kill-others 'npm run dev:hot' 'npm run dev:lint'",
    "dev:hot": "npm run clean:dev && node-hot",
    "dev:lint": "nodemon --watch src --watch .env src/app.ts --exec 'eslint src --cache'",
    "clean:dev": "rm -rf @dev && mkdir @dev",
    "clean:prod": "rm -rf dist && mkdir dist",
    "clean:prod:win": "rmdir /s /q dist && mkdir dist",
    "lint": "eslint src",
    "tsc": "npx tsc --noEmit",
    "build:webpack": "cross-env NODE_ENV=production webpack --progress ",
    "build": "npm run clean:prod && npm run lint && npm run build:webpack",
    "build:win": "npm run clean:prod:win && npm run lint && npm run build:webpack",
    "git:commit": "cd .. && npm run git:commit",
    "git:push": "cd .. && npm run git:push",
    "git:commit:push": "cd .. && npm run git:commit:push",
    "git:merge:mirror": "cd .. && npm run git:merge:mirror",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:restart": "pm2 delete ecosystem.config.js && pm2 start ecosystem.config.js",
    "pm2:reload": "pm2 reload ecosystem.config.js --force --update-env",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "install:prod": "npm install --only=prod",
    "reinstall": "npm run reinstall:module",
    "reinstall:module": "rm -rf node_modules && rm -f package-lock.json && npm i",
    "publish:development": "cd .. && npm run publish:development",
    "publish:staging": "cd .. && npm run publish:staging",
    "publish:production": "cd .. && npm run publish:production",
    "pub:dev": "cd .. && npm run pub:dev",
    "pub:staging": "cd .. && npm run pub:staging",
    "pub:prod": "cd .. && npm run pub:prod",
    "pub:all": "cd .. && npm run pub:all"
  },
  "dependencies": {
    "express": "^4.21.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@pdg/compare": "^1.0.6",
    "@pdg/data": "^1.0.1",
    "@pdg/date-time": "^1.0.2",
    "@pdg/types": "^1.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.22",
    "@types/express-session": "^1.18.1",
    "@types/node": "^22.15.29",
    "@types/node-schedule": "^2.1.8",
    "@types/webpack-env": "^1.18.8",
    "@typescript-eslint/parser": "^8.33.0",
    "agentkeepalive": "^4.6.0",
    "axios": "^1.11.0",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^9.1.2",
    "connect-redis": "^7.1.1",
    "cookie-parser": "^1.4.7",
    "core-js": "^3.42.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "ejs": "^3.1.10",
    "eslint": "9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "express-session": "^1.18.1",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "helmet": "^8.1.0",
    "http-proxy-middleware": "^3.0.5",
    "nocache": "^4.0.0",
    "node-hot-loader": "^1.23.0",
    "node-schedule": "^2.1.1",
    "nodemon": "^3.1.10",
    "path": "^0.12.7",
    "prettier": "^3.5.3",
    "redis": "^5.1.1",
    "ts-loader": "^9.5.2",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.33.0",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1"
  }
}
